<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="switchable_canvas"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="switchable_canvas">
                <items jcr:primaryType="cq:WidgetCollection">
                    <canvas
                        jcr:primaryType="cq:Widget"
                        anchor="100%"
                        fieldLabel="slider"
                        name="./canvas"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="nt:unstructured"
                            xtype="multifieldpanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <image
                                    jcr:primaryType="cq:Widget"
                                    anchor="100%"
                                    fieldLabel="Image"
                                    key="bigimage"
                                    xtype="pathfield"/>
                                <logo_icon
                                    jcr:primaryType="cq:Widget"
                                    anchor="100%"
                                    fieldLabel="Logo Icon"
                                    key="icon"
                                    xtype="pathfield"/>
                                <logo_title
                                    jcr:primaryType="cq:Widget"
                                    anchor="100%"
                                    fieldLabel="Logo Title"
                                    key="title"
                                    xtype="textfield"/>
                                <linkpath
                                    jcr:primaryType="cq:Widget"
                                    anchor="100%"
                                    fieldLabel="Link Path"
                                    key="link"
                                    xtype="pathfield"/>
                            </items>
                        </fieldConfig>
                    </canvas>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
